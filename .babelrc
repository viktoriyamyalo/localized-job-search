{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        ["module-resolver", {
          "root": ["./app"],
          "alias": {
            "components": "./components",
            "constants": "./constants",
            "common": "./components/common",
            "navigation": "./navigation",
            "screens": "./screens",
            "store": "./store",
            "reducers": "./reducers",
            "actions": "./actions"
          }
        }]
      ]
    }
  }
}
